{
    "name": "GALEJ/Leopardus",
    "description": "The Leopardus CMS/ERP/AIO.",
    "keywords": ["leopardus", "cms", "aio", "erp", "php", "framework", "laravel"],
    "license": "GNU",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "arcanedev/log-viewer": "^4.4",
        "bacon/bacon-qr-code": "~1.0.3",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "brotzka/laravel-dotenv-editor": "^2.0",
        "chumper/zipper": "^1.0",
        "fideloper/proxy": "~3.3",
        "intervention/image": "2.4.2",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laraveles/spanish": "^1.0",
        "maatwebsite/excel": "^3.1",
        "nwidart/laravel-modules": "^2.2",
        "paypal/rest-api-sdk-php": "^1.14",
        "pragmarx/google2fa-laravel": "^1.3",
        "spatie/laravel-permission": "^2.7"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        },
        "files": [
            "app/Helpers/auth.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover",
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
